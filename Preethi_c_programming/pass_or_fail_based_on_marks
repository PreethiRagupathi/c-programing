//c program to print pass if the mark is greater than 40
#include<stdio.h>
int main()
{
    int num;
    printf("enter num:");
    scanf("%d",&num);
    if(num>=40)
    {
        printf("pass");
    }
    else
    {
        printf("fail");
    }
    return 0;
}
******************************************************************************
OUTPUT:
enter num:67
pass
******************************************************************************
