//sum of natural numbers
#include <stdio.h>
void main()
{
    int n,sum=0;
    
    printf("Enter the number");
    scanf("%d",&n);
    
    for(int i=0;i<n;i++)
    {
        sum=sum+i;
    }
    printf("Sum of the given no is %d",sum);
}
****************************************************
OUTPUT:
enter the number:2
3
****************************************************
