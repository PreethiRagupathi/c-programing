#include<stdio.h>
#include<stdbool.h>
void main()
{
    int x,y;
    printf("enter x:");
    scanf("%d",&x);
    printf("enter y:");
    scanf("%d",&y);
    switch(x,y)
    {
        case 0:
        if(x==0&&y==0)
        {
            printf("%d %d lies in the origin",x,y);
        }
        else
        {
            printf("%d %d lies in the third quadrant",x,y);
        }
        case 1:
           printf("%d %d lies in the second quadrant",x,y);
        case 2:
        printf("%d %d lies in the fourth quadrant",x,y);
        case 3:
        printf("%d %d lies in the first quadrant",x,y);
    }
        
}
