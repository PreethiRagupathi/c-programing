#include<stdio.h>
int main()
{
    int n;
    printf("enter no of elements:");
    scanf("%d",&n);
    int prime[n];
    printf("enter elements");
    for( int i=0;i<n;i++)
    {
        scanf("%d",&prime[i]);
    }
        printf("values in the array");
    for(int i=0;i<n;i++){
        printf("values in the array %d",prime[i]);
    }
    return 0;
}
