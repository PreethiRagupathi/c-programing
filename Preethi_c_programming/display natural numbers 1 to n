#include<stdio.h>
int main() 
{
  int n;
  printf("enter no: ");
  scanf("%d",&n);
  for(int i=1;i<=n;i++)
  
      {
          printf("%d\n",i);
      }
  
     return 0;
}
**********************************************************
OUTPUT:
enter no: 6
1
2
3
4
5
6

*********************************************************
