//c program to print weather a given number is even number or not
#include<stdio.h>
int main()
{
    int num;
    printf("enter num:");
    scanf("%d",&num);
    if(num%2!=0)
    {
        printf("odd number");
    }
    else
    {
        printf("even number");
    }
    return 0;
}
*******************************************************************
OUTPUT:
enter num:8
even number
*******************************************************************
